{
    "root": "public/",
    "proxies": {
      "/api/": {
        "origin": "http://dev-atlas-tam.pantheonsite.io/api"
      }
    },
    "headers": {
      "/**": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "/**.css": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/**.js": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/static/**": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "/icons/*.png": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    "https_only": true,
    "error_page": "404.html"
  }